<h1>Listing leaderboards</h1>
<%= @leader_board.club.name %> @ <%= @leader_board.club.location %>

<table>
  <tr>
    <th>Name</th>
    <th>Elevation</th>
  </tr>
  <% @rides.each do |ride|%>
    <tr> 
    <td><%= ride.member.name%></td>
      <td><%= ride.total_elevation%></td>
     
    </tr>
  <% end %>
</table>
